{
  "name": "vue-qiankun-demo",
  "version": "1.0.0",
  "description": "一个通信的微前端",
  "main": "index.js",
  "scripts": {
    "install:subapp-ui": "cd subapp-ui && npm install",
    "install:subapp-login": "cd subapp-login && npm install",
    "install:subapp-blog": "cd subapp-blog && npm install",
    "install:master": "cd master && npm install",
    "install-all": "npm-run-all install:*",
    "start:subapp-ui": "cd subapp-ui && npm run serve ",
    "start:subapp-login": "cd subapp-login && npm run serve ",
    "start:subapp-blog": "cd subapp-blog && npm run serve ",
    "start:master": "cd master && npm run serve",
    "start-all": "npm-run-all --parallel start:*",
    "build:subapp-ui": "cd subapp-ui && npm run build",
    "build:subapp-login": "cd subapp-login && npm run build",
    "build:subapp-blog": "cd subapp-blog && npm run build",
    "build:master": "cd master && npm run build",
    "build-all": "npm-run-all --parallel build:*"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.4.0",
    "@vue/cli-service": "~5.0.8",
    "npm-run-all": "^4.1.5",
    "vue-template-compiler": "^2.6.11"
  }
}
